{
  "require": {
    "doctrine/orm": "2.6.3",
    "gedmo/doctrine-extensions": "2.4.31",
    "symfony/symfony": "4.3.*",
    "doctrine/annotations": "1.6.*",
    "doctrine/doctrine-bundle": "1.10.*",
    "friendsofsymfony/rest-bundle": "2.3.*",
    "sensio/framework-extra-bundle": "4.0.*",
    "jms/serializer-bundle": "2.4.*",
    "jms/aop-bundle": "1.3.0",
    "marc-mabe/php-enum": "4.*",
    "phpunit/phpunit": "8.*"
  },
  "autoload": {
    "psr-0": {
      "Uniwise": "../"
    },
    "classmap": [ "../Uniwise/Symfony/AppKernel.php" ]
  },
  "extra": {
    "symfony-app-dir": "/app/Uniwise/Symfony",
    "symfony-bin-dir": "/app/Uniwise/Symfony",
    "symfony-var-dir": "/app/Uniwise/Symfony",
    "symfony-web-dir": "/app/web",
    "symfony-tests-dir": "/app/tests",
    "symfony-assets-install": "relative"
  },
  "require-dev": {
    "symfony/maker-bundle": "^1.6"
  }
}
